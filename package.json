{
	"private": true,
	"name": "@kxphotographer/car-event-logger",
	"version": "0.0.1",
	"author": "kxphotographer <kxphotographer@users.noreply.github.com>",
	"license": "MIT",
	"scripts": {
		"build": "pnpm --recursive build",
		"check": "run-p --continue-on-error --aggregate-output check:* && pnpm --recursive check",
		"check:biome": "biome check ./",
		"check:cspell": "cspell ./",
		"clean": "pnpm --recursive --reverse clean",
		"format": "run-p --continue-on-error --aggregate-output format:*",
		"format:biome": "biome check ./ --write",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"cspell": "^9.6.4",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"npm-run-all": "^4.1.5"
	}
}
