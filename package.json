{
	"private": true,
	"name": "@kxphotographer/car-event-logger",
	"version": "0.0.1",
	"author": "kxphotographer <kxphotographer@users.noreply.github.com>",
	"license": "MIT",
	"scripts": {
		"build": "pnpm --recursive build",
		"check": "run-p --continue-on-error --aggregate-output check:*",
		"check:biome": "biome check ./",
		"check:cspell": "cspell ./",
		"clean": "pnpm --recursive --reverse clean",
		"format": "run-p --continue-on-error --aggregate-output format:*",
		"format:biome": "biome check ./ --write"
	},
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"cspell": "^9.6.4"
	},
	"dependencies": {
		"npm-run-all": "^4.1.5"
	}
}
