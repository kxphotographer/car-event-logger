{
	"private": true,
	"name": "@kxphotographer/car-event-logger-packages-domain",
	"version": "0.0.1",
	"scripts": {
		"build": "run-p --continue-on-error --aggregate-output build:*",
		"build:tsup": "tsup",
		"build:tsc-declaration": "tsc -p tsconfig.build.json --emitDeclarationOnly",
		"check": "run-p --continue-on-error --aggregate-output check:*",
		"check:typescript": "tsc --noEmit",
		"clean": "rimraf ./dist *.tsbuildinfo"
	},
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"default": "./dist/index.js"
	},
	"devDependencies": {
		"@faker-js/faker": "^10.3.0",
		"@kxphotographer/car-event-logger-packages-config-base": "workspace:*",
		"npm-run-all": "^4.1.5",
		"rimraf": "^6.1.3",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"vitest": "^4.0.18"
	}
}
